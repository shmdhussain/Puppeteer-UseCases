                            [300, 250]
                        ], 'div-gpt-ad-mpu-sidebar-desktop-1');
                    }
                    /*END: DESKTOP:: show mpu() for the sidebar in all sections except homepage*/

                    /*START: MOBILE:: show HALFPAGE after 2nd , 4th, 5th 6th component and after 1st in sidebar compoenent*/
                    if (deviceType == 'MOBILE') {
                        repeat_halfpage_section_comp();
                    }
                    /*END: MOBILE:: show HALFPAGE after 2nd , 4th, 5th 6th component and after 1st in sidebar compoenent*/


                    /*END: For Other Section pages than article page*/
                    break;
                case "technology":

                    /*START: DESKTOP:: leader board 1  size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/
                    if (deviceType == 'DESKTOP') {
                        ldb_bb_1([
                            [728, 90],
                            [970, 90],
                            [970, 250]
                        ]);
                    }
                    /*END: DESKTOP:: leader board 1  size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/

                    /*START: TABLET:: leader board 1  size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/
                    if (deviceType == 'TABLET') {
                        ldb_bb_1([
                            [728, 90]
                        ], 'div-gpt-ad-ldb-bb-tablet-1');
                    }
                    /*END: TABLET:: leader board 1  size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/


                    /*START: MOBILE:: leader board 1 size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/
                    if (deviceType == 'MOBILE') {
                        ldb_bb_1([
                            [320, 50]
                        ], 'div-gpt-ad-ldb-bb-mobile-sticky-1');
                    }
                    /*END: MOBILE:: leader board 1 size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/


                    /*START: Article Page ads Special cases*/
                    if (dfpConfig.page == 'Article') {
                        articlePageAds();
                        break; // For article page we need to show only desktop and mobile leader board 1 unit and repeat the logic
                    }
                    /*END: Article Page ads Special cases*/


                    /*START: For Other Section pages than article page*/

                    /*START: DESKTOP::  halfpage 1 and halfpage 2 in topstories 3rd and 5th topstories component*/
                    if (deviceType == 'DESKTOP') {
                        section_topstories_dfp_ad();
                    }
                    /*END: DESKTOP::  halfpage 1 and halfpage 2 in topstories 3rd and 5th topstories component*/

                    /*START: DESKTOP:: leader board 2  size range and ldb_bb_2(sizeArrObj, slotDivId<optional>)*/
                    if (deviceType == 'DESKTOP') {
                        if (viewportWidth >= smallDesktopMinWidth && viewportWidth <= smallDesktopMaxWidth) {
                            console.log("ldb 2 small");
                            ldb_bb_2([
                                [728, 90]
                            ]);
                        } else {
                            console.log("ldb 2 big");

                            ldb_bb_2([
                                [728, 90],
                                [970, 90],
                                [970, 250]
                            ]);
                        }
                    }
                    /*END: DESKTOP:: leader board 2  size range and ldb_bb_2(sizeArrObj, slotDivId<optional>)*/

                    /*START: TABLET:: Leader Board2 - place ldb2 after bottom of every component except first comp - tablet*/
                    if (deviceType == 'TABLET') {
                        repeat_ldb_bb_ad_section_comp();
                    }
                    /*END: TABLET:: Leader Board2 - place ldb2 after bottom of every component except first comp - tablet*/

                    /*START:  MOBILE:: leader board 2 mobile size range and ldb_bb_2(sizeArrObj, slotDivId<optional>)*/
                    if (deviceType == 'MOBILE') {
                        ldb_bb_2([
                            [320, 50]
                        ], 'div-gpt-ad-ldb-bb-mobile-2');
                    }
                    /*END:  MOBILE:: leader board 2 mobile size range and ldb_bb_2(sizeArrObj, slotDivId<optional>)*/



                    /*START: DESKTOP:: show mpu() for the sidebar in all sections except homepage*/
                    if (deviceType == 'DESKTOP') {
                        mpu_1([
                            [300, 250]
                        ], 'div-gpt-ad-mpu-sidebar-desktop-1');
                    }
                    /*END: DESKTOP:: show mpu() for the sidebar in all sections except homepage*/

                    /*START: MOBILE:: show HALFPAGE after 2nd , 4th, 5th 6th component and after 1st in sidebar compoenent*/
                    if (deviceType == 'MOBILE') {
                        repeat_halfpage_section_comp();
                    }
                    /*END: MOBILE:: show HALFPAGE after 2nd , 4th, 5th 6th component and after 1st in sidebar compoenent*/


                    /*END: For Other Section pages than article page*/
                    break;
                case "varieties":

                    /*START: DESKTOP:: leader board 1  size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/
                    if (deviceType == 'DESKTOP') {
                        ldb_bb_1([
                            [728, 90],
                            [970, 90],
                            [970, 250]
                        ]);
                    }
                    /*END: DESKTOP:: leader board 1  size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/

                    /*START: TABLET:: leader board 1  size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/
                    if (deviceType == 'TABLET') {
                        ldb_bb_1([
                            [728, 90]
                        ], 'div-gpt-ad-ldb-bb-tablet-1');
                    }
                    /*END: TABLET:: leader board 1  size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/


                    /*START: MOBILE:: leader board 1 size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/
                    if (deviceType == 'MOBILE') {
                        ldb_bb_1([
                            [320, 50]
                        ], 'div-gpt-ad-ldb-bb-mobile-sticky-1');
                    }
                    /*END: MOBILE:: leader board 1 size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/


                    /*START: Article Page ads Special cases*/
                    if (dfpConfig.page == 'Article') {
                        articlePageAds();
                        break; // For article page we need to show only desktop and mobile leader board 1 unit and repeat the logic
                    }
                    /*END: Article Page ads Special cases*/


                    /*START: For Other Section pages than article page*/

                    /*START: DESKTOP::  halfpage 1 and halfpage 2 in topstories 3rd and 5th topstories component*/
                    if (deviceType == 'DESKTOP') {
                        section_topstories_dfp_ad();
                    }
                    /*END: DESKTOP::  halfpage 1 and halfpage 2 in topstories 3rd and 5th topstories component*/

                    /*START: DESKTOP:: leader board 2  size range and ldb_bb_2(sizeArrObj, slotDivId<optional>)*/
                    if (deviceType == 'DESKTOP') {
                        if (viewportWidth >= smallDesktopMinWidth && viewportWidth <= smallDesktopMaxWidth) {
                            console.log("ldb 2 small");
                            ldb_bb_2([
                                [728, 90]
                            ]);
                        } else {
                            console.log("ldb 2 big");

                            ldb_bb_2([
                                [728, 90],
                                [970, 90],
                                [970, 250]
                            ]);
                        }
                    }
                    /*END: DESKTOP:: leader board 2  size range and ldb_bb_2(sizeArrObj, slotDivId<optional>)*/

                    /*START: TABLET:: Leader Board2 - place ldb2 after bottom of every component except first comp - tablet*/
                    if (deviceType == 'TABLET') {
                        repeat_ldb_bb_ad_section_comp();
                    }
                    /*END: TABLET:: Leader Board2 - place ldb2 after bottom of every component except first comp - tablet*/

                    /*START:  MOBILE:: leader board 2 mobile size range and ldb_bb_2(sizeArrObj, slotDivId<optional>)*/
                    if (deviceType == 'MOBILE') {
                        ldb_bb_2([
                            [320, 50]
                        ], 'div-gpt-ad-ldb-bb-mobile-2');
                    }
                    /*END:  MOBILE:: leader board 2 mobile size range and ldb_bb_2(sizeArrObj, slotDivId<optional>)*/



                    /*START: DESKTOP:: show mpu() for the sidebar in all sections except homepage*/
                    if (deviceType == 'DESKTOP') {
                        mpu_1([
                            [300, 250]
                        ], 'div-gpt-ad-mpu-sidebar-desktop-1');
                    }
                    /*END: DESKTOP:: show mpu() for the sidebar in all sections except homepage*/

                    /*START: MOBILE:: show HALFPAGE after 2nd , 4th, 5th 6th component and after 1st in sidebar compoenent*/
                    if (deviceType == 'MOBILE') {
                        repeat_halfpage_section_comp();
                    }
                    /*END: MOBILE:: show HALFPAGE after 2nd , 4th, 5th 6th component and after 1st in sidebar compoenent*/


                    /*END: For Other Section pages than article page*/
                    break;


                default:

                    /*START: Special case when there is new section*/
                    
                    
                    if(getUnknownSection()){
                        // debugger;
                        /*START: DESKTOP:: leader board 1  size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/
                        if (deviceType == 'DESKTOP') {
                            ldb_bb_1([
                                [728, 90],
                                [970, 90],
                                [970, 250]
                            ]);
                        }
                        /*END: DESKTOP:: leader board 1  size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/

                        /*START: TABLET:: leader board 1  size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/
                        if (deviceType == 'TABLET') {
                            ldb_bb_1([
                                [728, 90]
                            ], 'div-gpt-ad-ldb-bb-tablet-1');
                        }
                        /*END: TABLET:: leader board 1  size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/


                        /*START: MOBILE:: leader board 1 size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/
                        if (deviceType == 'MOBILE') {
                            ldb_bb_1([
                                [320, 50]
                            ], 'div-gpt-ad-ldb-bb-mobile-sticky-1');
                        }
                        /*END: MOBILE:: leader board 1 size range and ldb_bb_1(sizeArrObj, slotDivId<optional>)*/


                        /*START: Article Page ads Special cases*/
                        if (dfpConfig.page == 'Article') {
                            articlePageAds();
                            break; // For article page we need to show only desktop and mobile leader board 1 unit and repeat the logic
                        }
                        /*END: Article Page ads Special cases*/


                        /*START: For Other Section pages than article page*/

                        /*START: DESKTOP::  halfpage 1 and halfpage 2 in topstories 3rd and 5th topstories component*/
                        if (deviceType == 'DESKTOP') {
                            section_topstories_dfp_ad();
                        }
                        /*END: DESKTOP::  halfpage 1 and halfpage 2 in topstories 3rd and 5th topstories component*/

                        /*START: DESKTOP:: leader board 2  size range and ldb_bb_2(sizeArrObj, slotDivId<optional>)*/
                        if (deviceType == 'DESKTOP') {
                            if (viewportWidth >= smallDesktopMinWidth && viewportWidth <= smallDesktopMaxWidth) {
                                console.log("ldb 2 small");
                                ldb_bb_2([
                                    [728, 90]
                                ]);
                            } else {
                                console.log("ldb 2 big");

                                ldb_bb_2([
                                    [728, 90],
                                    [970, 90],
                                    [970, 250]
                                ]);
                            }
                        }
                        /*END: DESKTOP:: leader board 2  size range and ldb_bb_2(sizeArrObj, slotDivId<optional>)*/

                        /*START: TABLET:: Leader Board2 - place ldb2 after bottom of every component except first comp - tablet*/
                        if (deviceType == 'TABLET') {
                            repeat_ldb_bb_ad_section_comp();
                        }
                        /*END: TABLET:: Leader Board2 - place ldb2 after bottom of every component except first comp - tablet*/

                        /*START:  MOBILE:: leader board 2 mobile size range and ldb_bb_2(sizeArrObj, slotDivId<optional>)*/
                        if (deviceType == 'MOBILE') {
                            ldb_bb_2([
                                [320, 50]
                            ], 'div-gpt-ad-ldb-bb-mobile-2');
                        }
                        /*END:  MOBILE:: leader board 2 mobile size range and ldb_bb_2(sizeArrObj, slotDivId<optional>)*/



                        /*START: DESKTOP:: show mpu() for the sidebar in all sections except homepage*/
                        if (deviceType == 'DESKTOP') {
                            mpu_1([
                                [300, 250]
                            ], 'div-gpt-ad-mpu-sidebar-desktop-1');
                        }
                        /*END: DESKTOP:: show mpu() for the sidebar in all sections except homepage*/

                        /*START: MOBILE:: show HALFPAGE after 2nd , 4th, 5th 6th component and after 1st in sidebar compoenent*/
                        if (deviceType == 'MOBILE') {
                            repeat_halfpage_section_comp();
                        }
                        /*END: MOBILE:: show HALFPAGE after 2nd , 4th, 5th 6th component and after 1st in sidebar compoenent*/


                        /*END: For Other Section pages than article page*/

                    }
                    /*END: Special case when there is new section*/
            }

        },

        $get: function() {
            return {

            };
        }

    };

}]);