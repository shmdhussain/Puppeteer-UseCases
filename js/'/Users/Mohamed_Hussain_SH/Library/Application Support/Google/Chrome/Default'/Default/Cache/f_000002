                    <a data-ng-href="{{'#' + tab.type}}_TAB" class="" data-ng-bind="tab.displayName" data-ng-click="showSectionFilter($event, tab);trackGAEvent('SECTION_TABS',tab.displayName);" data-ng-class="{'icon-arrow-down': tab.sections && tab.sections.length > 0 && !dropDown, 'icon-arrow-up': tab.sections && tab.sections.length > 0 && dropDown}"></a>
                    <ul data-ng-if="tab.sections" class="tab-sections">
                        <li data-ng-repeat="section in tab.sections" data-ng-click="filterStoriesByCategory(tab)">
                            <input id="{{section.name}}" name="{{section.name}}" value="{{section.name}}" data-ng-click="$event.stopPropagation();filterStoriesByCategory(tab);updateSelection();" data-ng-model="selectedSections[$index]" data-ng-false-value="''" data-ng-true-value="'{{section.name}}'" type="checkbox" name="section" class="section-check" data-ng-checked="{{section.name == sectionUrl}}"><label data-ng-bind="section.displayName" data-ng-click="filterStoriesByCategory(tab)" for="{{section.name}}"></label>
                         </li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
