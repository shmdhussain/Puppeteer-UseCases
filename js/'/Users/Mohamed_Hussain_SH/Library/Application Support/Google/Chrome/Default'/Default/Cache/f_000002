<!-- START: Safe Calling of FB Pixel function -->
<script type="text/javascript">
    window.fbqSafeCallQueue = [];
    window.fbqSafeCallTimerClearTimeoutId;
    window.pushInFbqSafeCallQueue = function(callback){
        if(window.fbq){
            callback();
        }else{
            fbqSafeCallQueue.push(callback);
        }
    };

    window.fbqSafeCall= function(){
        if(window.fbq){
            clearTimeout(window.fbqSafeCallTimerClearTimeoutId);
            for (var i = 0; i < window.fbqSafeCallQueue.length; i++) {
                window.fbqSafeCallQueue[i]();
            }
            window.fbqSafeCallQueue = [];
        }else{
            window.fbqSafeCallTimerClearTimeoutId = setTimeout(function(){
                window.fbqSafeCall();
            }, 3000);
        }
    };
    window.fbqSafeCall();

</script>
<!-- END: Safe Calling of FB Pixel function -->


